= form_for(:user, url: new_user_session_path) do |f|
  .field
    = f.label :email
    = f.email_field :email, autofocus: true

  .field
    = f.label :password
    = f.password_field :password, autocomplete: "off"

  .field
    = f.check_box :remember_me
    = f.label :remember_me

  .actions
    = f.submit "Log in"
